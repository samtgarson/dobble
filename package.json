{
  "name": "example-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev --port 5000",
    "build": "next build",
    "start": "next start --port 5000",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "test": "jest"
  },
  "dependencies": {
    "@google-cloud/firestore": "^4.9.8",
    "@harelpls/use-pusher": "^7.2.1",
    "@supabase/supabase-js": "^1.7.1",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "axios": "^0.21.1",
    "bulma": "^0.7.5",
    "classnames": "^2.2.6",
    "consola": "^2.15.3",
    "cookie": "^0.4.1",
    "date-fns": "^2.19.0",
    "fathom-client": "^3.0.0",
    "firebase": "^8.3.1",
    "firebase-admin": "^9.5.0",
    "framer-motion": "^4.0.3",
    "jsonwebtoken": "^8.5.1",
    "next": "^10.0.9",
    "next-compose-plugins": "^2.2.1",
    "pusher": "^5.0.0",
    "rbx": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "source-map-support": "^0.5.19",
    "unstated-next": "^1.1.0",
    "use-supabase": "^1.0.3",
    "uuid": "^8.3.2",
    "yawg": "^1.2.3"
  },
  "devDependencies": {
    "@samtgarson/eslint-config": "^2.4.0",
    "@types/classnames": "^2.2.11",
    "@types/cookie": "^0.4.0",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.36",
    "@types/react": "^17.0.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "sass": "^1.32.8",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "eslintConfig": {
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "extends": [
      "plugin:react/recommended",
      "@samtgarson/eslint-config",
      "@samtgarson/eslint-config/typescript"
    ],
    "rules": {
      "@typescript-eslint/no-non-null-assertion": 0,
      "react/prop-types": 0,
      "react/jsx-wrap-multilines": [
        "error",
        {
          "declaration": "parens-new-line",
          "assignment": "parens-new-line",
          "return": "parens-new-line",
          "arrow": "parens-new-line",
          "condition": "ignore",
          "logical": "ignore",
          "prop": "ignore"
        }
      ],
      "import/extensions": [
        "error",
        {
          "scss": "always"
        }
      ]
    }
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".+\\.spec\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^~/(.+)$": "<rootDir>/$1"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".next",
      ".now"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    }
  }
}
